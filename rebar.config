{deps, [
    {lager, "3.9.2"},
    {eredis, ".*", {git, "https://github.com/silviucpp/eredis", "d447fb2e0142270eed7a0cf3d3b850fce6773371"}},
    {erlpool, ".*", {git, "https://github.com/silviucpp/erlpool.git", {tag, "v1.4.1"}}},
    {jchash, ".*", {git, "https://github.com/cabol/jchash.git", {tag, "v0.1.3"}}},
    {erl_hash, ".*", {git, "https://github.com/silviucpp/erl_hash.git", {tag, "1.0.5"}}}
]}.

{erl_opts, [
    {parse_transform,lager_transform},
    warn_unused_vars,
    warn_shadow_vars,
    warn_unused_import,
    warn_unused_function,
    warn_bif_clash,
    warn_unused_record,
    warn_deprecated_function,
    warn_obsolete_guard,
    strict_validation,
    warn_export_vars,
    warn_exported_vars,
    warn_export_all,
    warnings_as_errors
]}.
